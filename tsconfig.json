{
    "compilerOptions": {
        "outDir": "./dist/",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "module": "esnext",
        "target": "es5",
        "jsx": "react",
        "downlevelIteration": true,
        "allowJs": true
    },
    "exclude": ["spec"]
}